{
  "name": "poll-app",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "babel-loader": "7.1.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.26.0",
    "jasmine-core": "2.6.4",
    "karma": "1.7.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "1.1.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.4",
    "lighthouse": "2.2.1",
    "material-ui": "0.17.4",
    "react-redux": "5.0.5",
    "react-router": "4.1.1",
    "react-router-dom": "4.1.1",
    "react-router-redux": "4.0.8",
    "react-scripts": "0.9.5",
    "react-tap-event-plugin": "2.0.1",
    "redux": "3.6.0",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.2.0",
    "source-map-explorer": "1.4.0"
  },
  "dependencies": {
    "firebase": "3.8.0",
    "firebase-functions": "0.5.9",
    "firebase-tools": "3.6.1",
    "history": "4.6.1",
    "immutable": "3.8.1",
    "jquery": "3.2.1",
    "lodash": "4.17.4",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-router": "4.1.1",
    "react-router-redux": "5.0.0-alpha.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && npm run build:sw",
    "build:sw": "find ./public -name '*-sw.js' -exec cp {} ./build ';'",
    "test": "react-scripts test --env=jsdom",
    "report:performance": "./node_modules/.bin/lighthouse 'https://poll-app-67a23.firebaseapp.com' --output json --output html --output-path ./reports/performance-report.json",
    "report:test": "npm test -- --coverage",
    "report:js": "./node_modules/.bin/source-map-explorer build/static/js/main.*",
    "report:coverage": "cat ./coverage/lcov.info | ./node_modules/.bin/codacy-coverage",
    "karma-test": "karma start ",
    "eject": "react-scripts eject",
    "deploy": "./node_modules/.bin/firebase deploy"
  }
}

{
  "name": "poll-app",
  "version": "0.0.1",
  "private": true,
  "homepage": "https://fif0o.github.io/poll-app/storybook-static",
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "babel-loader": "7.1.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.26.0",
    "codeceptjs": "1.0.3",
    "enzyme": "3.0.0",
    "enzyme-adapter-react-15": "1.0.0",
    "enzyme-redux": "0.1.7",
    "fetch-mock": "5.13.1",
    "firebase-mock": "1.1.5",
    "getstorybook": "1.7.0",
    "gh-pages": "1.0.0",
    "jasmine-core": "2.6.4",
    "jsdom": "11.3.0",
    "karma": "1.7.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "1.1.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.4",
    "lighthouse": "2.2.1",
    "material-ui": "0.17.4",
    "nock": "9.0.22",
    "react-redux": "5.0.5",
    "react-router": "4.1.1",
    "react-router-dom": "4.1.1",
    "react-router-redux": "4.0.8",
    "react-scripts": "1.0.0",
    "react-tap-event-plugin": "2.0.1",
    "react-test-renderer": "15.6.2",
    "redux": "3.6.0",
    "redux-logger": "3.0.6",
    "redux-mock-store": "1.3.0",
    "redux-test-utils": "0.1.3",
    "redux-thunk": "2.2.0",
    "source-map-explorer": "1.4.0",
    "webdriver-manager": "12.0.6",
    "webdriverio": "4.9.5"
  },
  "dependencies": {
    "codacy-coverage": "2.0.3",
    "dotenv": "4.0.0",
    "firebase": "3.8.0",
    "firebase-functions": "0.5.9",
    "firebase-tools": "3.10.1",
    "history": "4.6.1",
    "immutable": "3.8.1",
    "jquery": "3.2.1",
    "lodash": "4.17.4",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-router": "4.1.1",
    "react-router-redux": "5.0.0-alpha.6",
    "source-map-explorer": "1.4.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && npm run build:sw",
    "build:sw": "find ./public -name '*-sw.js' -exec cp {} ./build ';'",
    "test": "react-scripts test -u",
    "bdd:install-selenium": "./node_modules/.bin/webdriver-manager update",
    "bdd:server": "npm run bdd:install-selenium && ./node_modules/.bin/webdriver-manager start --standalone",
    "bdd:run":"./node_modules/.bin/codeceptjs run --steps",
    "report:performance": "./node_modules/.bin/lighthouse 'https://poll-app-67a23.firebaseapp.com' --output json --output html --output-path ./reports/performance-report.json",
    "report:test": "npm test -- --coverage --collectCoverageFrom=src/**/*.js --collectCoverageFrom=!src/story --collectCoverageFrom=!bdd/**/*",
    "report:js": "./node_modules/.bin/source-map-explorer build/static/js/main.*",
    "report:coverage": "cat ./coverage/lcov.info | ./node_modules/.bin/codacy-coverage",
    "karma-test": "karma start ",
    "eject": "react-scripts eject",
    "deploy": "./node_modules/.bin/firebase deploy",
    "deploy:storybook": "npm run build-storybook && ./node_modules/.bin/gh-pages -d storybook-static",
    "analyze": "source-map-explorer build/static/js/main.*",
    "ie11": "box_name='win7-ie11' vagrant up",
    "ie11:open": "open http://10.0.2.2:3000",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  }
}
